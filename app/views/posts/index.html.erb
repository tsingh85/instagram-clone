<%= stylesheet_link_tag("post")%>

<p id="notice"><%= notice %></p>

<div id='posts', class="text-center">
  <% @posts.each do |post| %>
    <div class="post-container">
      <h2><%= post.title %></h2>
      <p><%= post.description %></p>
      <p><%= post.keywords %></p>
      <p style= "margin:0"  ><%= link_to '', post, class:"post-link" %></p>
    </div>
  <% end %>
</div>